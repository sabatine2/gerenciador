  <mapping class="com.midiasocial.model.UsuarioAppMidiaSocial"/>
  <mapping class="com.midiasocial.model.UsuarioPubMidiaSocial"/>
  <mapping class="com.midiasocial.model.Publicacao"/>
  <mapping class="com.midiasocial.model.Comentario"/>
